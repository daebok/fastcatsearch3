<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!--
사용법 : service/searchProfile.json?queryProfile=mainSearch&keyword1=발매트&keyword2=&rankingProfile=rankingA&page=1
-->
<profiles>
    <query>
        <!--sn,ln은 page=1,2,3 등으로 사용해야 한다.-->
        <query-profile id="mainSearch" name="메인창 검색" description="메인창검색쿼리 프로파일">
            <entry type="cn" value="v1,v2,v3,v4,v5" />
            <entry type="se" value="{title:#keyword1:10:15}OR{body:#keyword1:5:15}" />
            <entry type="ft" value="" />
            <entry type="gr" value="" />
            <entry type="ra" value="" />
            <entry type="pageSize" value="50" />
        </query-profile>
    </query>
    <ranking-list>
        <ranking-profile id="rankingA" name="랭킹A" description="카테고리점수와 클릭점수 부스팅을 수행한다.">
            <keyword-boosting-list>
                <keyword-boosting order="1" id="CateBoost">
                    <dictionary id="cat_boost" plugin="Product" keyColumn="keyword" valueColumn="value" keyword="keyword2"/>
                    <boosting-filter fieldId="cat1;cat2" score="100000"/>
                </keyword-boosting>
            </keyword-boosting-list>
            <search-boosting-list>
                <search-boosting order="1" id="RS1">
                    <target-collection theSame="true"/>
                    <search-field id="makerBrand" type="ALL" option="15" score="1000000" keyword="keyword2"/>
                    <option remove-keyword="true"/>
                </search-boosting>
                <search-boosting order="2" id="RS5">
                    <target-collection theSame="false" collectionId="clickLog" joinField="click_id" scoreField="click_score"/>
                    <search-field id="modelWeight" type="ALL" option="3" keyword="keyword2"/>
                    <option remove-keyword="false"/>
                </search-boosting>
            </search-boosting-list>
        </ranking-profile>
    </ranking-list>
</profiles>